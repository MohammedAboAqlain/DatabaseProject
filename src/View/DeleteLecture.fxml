<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="rootPane" prefHeight="380.0" prefWidth="409.0" style="-fx-background-color: gray;" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.DeleteLecture">
    <children>
        <Label layoutX="126.0" layoutY="21.0" prefHeight="48.0" prefWidth="157.0" text="حذف محاضرة" textAlignment="CENTER" textFill="#092486">
            <font>
                <Font name="System Bold" size="24.0" />
            </font>
        </Label>

        <Button layoutX="343.0" layoutY="341.0" mnemonicParsing="false" onAction="#onBackBtn" style="-fx-background-color: #092486;" text="رجوع" textFill="WHITE">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Button>
        <Button layoutX="256.0" layoutY="200.0" mnemonicParsing="false" onAction="#onDeleteLectureBtn" style="-fx-background-color: #092486;" text="حذف" textFill="WHITE">
            <font>
                <Font name="System Bold" size="12.0" />
            </font>
        </Button>
        <Label layoutX="309.0" layoutY="157.0" text="رقم المحاضرة" textFill="#092486">
            <font>
                <Font name="System Bold" size="12.0" />
            </font></Label>
        <ComboBox fx:id="lectureNumber" layoutX="138.0" layoutY="153.0" prefHeight="25.0" prefWidth="157.0" promptText="رقم المحاضرة" />

        <Label layoutX="312.0" layoutY="110.0" text="اسم المجلس" textFill="#092486">
            <font>
                <Font name="System Bold" size="12.0" />
            </font></Label>
        <ComboBox fx:id="councilName" layoutX="62.0" layoutY="106.0" onAction="#initializeFields" prefHeight="25.0" prefWidth="236.0" promptText="اختر اسم المجلس المراد حذف محاضرته" />
        
        <Label fx:id="statusLabel" layoutX="52.0" layoutY="259.0" prefHeight="36.0" prefWidth="329.0" />
    </children>
</AnchorPane>
