        <Label layoutX="83.0" layoutY="23.0" prefHeight="48.0" prefWidth="230.0" text="إضافة وتحرير محاضرات" textAlignment="CENTER" textFill="#092486">
            <font>
                <Font name="System Bold" size="24.0" />
            </font>
        </Label>
        <Button layoutX="114.0" layoutY="121.0" mnemonicParsing="false" onAction="#onAddLecture" prefHeight="37.0" prefWidth="182.0" style="-fx-background-color: #092486;" text="إضافة محاضرة" textFill="WHITE">
