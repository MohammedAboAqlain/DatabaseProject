<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="rootPane" prefHeight="375.0" prefWidth="406.0" style="-fx-background-color: gray;" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.UserLogin">
   <children>

      <TextField fx:id="userName" layoutX="110.0" layoutY="127.0" promptText="أدخل اسم المستخدم" />
      <PasswordField fx:id="password" layoutX="110.0" layoutY="174.0" promptText="أدخل كلمة المرور" />

      <Label layoutX="287.0" layoutY="127.0" prefHeight="25.0" prefWidth="84.0" text="اسم المستخدم" textFill="#092486">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Label layoutX="287.0" layoutY="174.0" prefHeight="25.0" prefWidth="84.0" text="كلمة المرور" textFill="#092486">
         <font>
            <Font name="System Bold" size="12.0" />
         </font></Label>
       <Label layoutX="120.0" layoutY="44.0" prefHeight="44.0" prefWidth="241.0" text="تسجيل الدخول" textAlignment="CENTER" textFill="#092486">
           <font>
               <Font name="System Bold" size="28.0" />
           </font>
       </Label>
      <Button layoutX="255.0" layoutY="220.0" mnemonicParsing="false" onAction="#login" prefHeight="25.0" prefWidth="44.0" style="-fx-background-color: #092486;" text="تأكيد" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
      <Label fx:id="statusLabel" layoutX="25.0" layoutY="267.0" prefHeight="36.0" prefWidth="357.0" />
      <Button layoutX="325.0" layoutY="336.0" mnemonicParsing="false" onAction="#onBackBtn" prefHeight="25.0" prefWidth="57.0" style="-fx-background-color: #092486;" text="رجوع" textFill="WHITE">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
